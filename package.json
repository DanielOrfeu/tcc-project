{
  "name": "treina-mentes",
  "version": "1.0.0",
  "description": "Projeto TCC denominado TreinaMentes, destinado à auxiliar crianças com TANV. Criado por Daniel Orfeu e Thiago Lemos",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build -c electron && electron .",
    "electron-build": "ng build --prod && electron .",
    "package-mac": "electron-packager . treina-mentes --overwrite --platform=darwin --arch=x64 --icon=icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . treina-mentes --overwrite --asar --platform=win32 --arch=ia32 --icon=icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"TreinaMentes TCC\"",
    "package-linux": "electron-packager . treina-mentes --overwrite --asar --platform=linux --arch=x64 --icon=icons/icon.png --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/cdk": "~9.1.2",
    "@angular/common": "~9.0.6",
    "@angular/compiler": "~9.0.6",
    "@angular/core": "~9.0.6",
    "@angular/forms": "~9.0.6",
    "@angular/material": "~9.1.2",
    "@angular/platform-browser": "~9.0.6",
    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/router": "~9.0.6",
    "angular-intro.js": "^3.4.0",
    "electron-packager": "^14.2.1",
    "intro.js": "^2.9.3",
    "ng-connection-service": "^1.0.4",
    "ng-dimmer": "^0.1.6",
    "ngx-spinner": "^9.0.2",
    "ngx-toastr": "^12.0.1",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular/cli": "~9.0.6",
    "@angular/compiler-cli": "~9.0.6",
    "@angular/language-service": "~9.0.6",
    "@types/intro.js": "^2.4.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.39",
    "codelyzer": "^5.1.2",
    "electron": "^8.1.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}